        valueLabel.Font = Enum.Font.GothamBold
        valueLabel.TextSize = 12
        valueLabel.TextXAlignment = Enum.TextXAlignment.Right
        valueLabel.Name = "Value"
        valueLabel.Parent = frame
        
        return valueLabel
    end
    
    -- –°–æ–∑–¥–∞–µ–º —ç–ª–µ–º–µ–Ω—Ç—ã —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∏
    local incomeStat = createStatItem("–î–æ—Ö–æ–¥:", "$0.00", Color3.fromRGB(100, 255, 100), UDim2.new(0, 0, 0, 0))
    local propertiesStat = createStatItem("–û–±—ä–µ–∫—Ç—ã:", "0", Color3.fromRGB(100, 200, 255), UDim2.new(0.5, 5, 0, 0))
    local occupancyStat = createStatItem("–ó–∞–ø–æ–ª–Ω–µ–Ω–æ:", "0%", Color3.fromRGB(255, 200, 100), UDim2.new(0, 0, 0, 30))
    local cycleStat = createStatItem("–¶–∏–∫–ª:", "#0", Color3.fromRGB(200, 100, 255), UDim2.new(0.5, 5, 0, 30))
    local changeStat = createStatItem("–ò–∑–º–µ–Ω–µ–Ω–∏–µ:", "+$0.00", Color3.fromRGB(255, 255, 100), UDim2.new(0, 0, 0, 60))
    local timeStat = createStatItem("–í—Ä–µ–º—è:", "0.00s", Color3.fromRGB(100, 255, 255), UDim2.new(0.5, 5, 0, 60))
    
