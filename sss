    BestInfo.Position = UDim2.new(0, 10, 0, 25)
    BestInfo.BackgroundTransparency = 1
    BestInfo.Text = "–ó–∞–≥—Ä—É–∑–∫–∞..."
    BestInfo.TextColor3 = Color3.fromRGB(200, 200, 220)
